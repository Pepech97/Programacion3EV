import java.util.Arrays;

public class Vagon {
	private Integer num;
	private int[] asientos;
	private static Integer siguiente=1;
	
	public Vagon() {
		this.num = siguiente;
		this.asientos = new int [10];
		siguiente++;
	}

	@Override
	public String toString() {
		return "Vagon [num=" + num + ", asientos=" + Arrays.toString(asientos) + "]";
	}
	
	
}
