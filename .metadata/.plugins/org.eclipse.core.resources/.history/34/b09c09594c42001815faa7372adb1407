import java.util.*;

public class GestionUsuarios {

	public static void main(String[] args) {
		TreeMap<String,String> usuarios = new TreeMap<>();
		boolean usuCorrecto = false;
		do {
			String usuario=Leer.pedirCadena("Introduce el usuario:");
			for (int i = 0; i < usuario.length(); i++) {
				char charusuario=usuario.charAt(i);
				if ((charusuario<'a' || charusuario >'z')&&(charusuario<'A' || charusuario >'Z') && (charusuario>'1' || charusuario<'9')) {
					System.out.println("holaquetal");
				}
			}
		} while (usuCorrecto==false);
		
		
	}

}
